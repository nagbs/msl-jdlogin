.popup-wrapper{position:fixed; left:0; top:0; width:100%; height:100%; background:#ccc; z-index:1000; opacity:0.8;}
.pop-up-container {background:#000; color:#fff; padding:10px; width:500px; -webkit-border-radius:10px; -moz-border-radius:10px; border-radius:10px; -webkit-box-shadow:0 0 14px rgba(50, 50, 50, 0.79); -moz-box-shadow:0 0 14px rgba(50, 50, 50, 0.79); box-shadow:0 0 14px rgba(50, 50, 50, 0.79); opacity:0.9; filter:alpha(opacity=90); display:table-cell; vertical-align:middle;}
.pop-up-container h2 {margin:0 0 20px; color:#fff; font-size:18px;}
.pop-up-container .pop-row {display:block; margin:0 0 15px; float:left; width:100%;}
.pop-up-container .pop-row .lables {width:40%; float:left; font-size:13px; text-align:right;}
.pop-up-container .pop-row .input-box  {width:57%; float:right; font-size:14px; text-align:left;}

